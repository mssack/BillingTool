<UserControl x:Class="CsWpfBase.Db.tools.controls.CsDbTableEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CsWpfBase.Db.tools.controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" x:Name="UserControl">
	<Grid DataContext="{Binding ElementName=UserControl, Path=ItemSource}">
		<DataGrid ItemsSource="{Binding}" />
    </Grid>
</UserControl>
